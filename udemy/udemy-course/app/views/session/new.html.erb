<h1>Login</h1>
<%= form_with(scope: :session, url:login_path) do |form| %>

  <div>
    <%= form.label :username, style: "display: block" %>
    <%= form.text_field :username %>
    <%= form.label :password, style: "display: block" %>
    <%= form.password_field :password %>

  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
